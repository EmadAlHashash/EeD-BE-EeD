<!DOCTYPE html>
<html class="dark" lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewData["Title"] - Eed_B_Eed</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Be+Vietnam+Pro:wght@400;500;700;900&display=swap"
          rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Eed_B_Eed.styles.css" asp-append-version="true" />
    @RenderSection("Styles", required: false)

</head>

<body>
    <div class="site">
        <header class="site-header">
            <div  class="brand" asp-area="" asp-controller="Home" asp-action="Index">
                <svg class="logo" viewBox="0 0 48 48" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <g clip-path="url(#clip0_6_330)">
                        <path fill-rule="evenodd" clip-rule="evenodd"
                              d="M24 0.757355L47.2426 24L24 47.2426L0.757355 24L24 0.757355ZM21 35.7574V12.2426L9.24264 24L21 35.7574Z"
                              fill="currentColor" />
                    </g>
                    <defs>
                        <clipPath id="clip0_6_330">
                            <rect width="48" height="48" fill="white" />
                        </clipPath>
                    </defs>
                </svg>
                <a asp-area="" asp-controller="Home" asp-action="Index" class="name">
                    <h2 class="name">SkillSwap</h2>
                    
                </a>

            </div>

            <nav class="nav">
                <a asp-area="" asp-controller="Home" asp-action="About">About Us</a>
                <a asp-area="" asp-controller="Home" asp-action="Browse">Browse</a>
                <a asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
            </nav>
            <div class="actions">
                <button class="btn" type="button">
                    @await Html.PartialAsync("_LoginPartial")
                    </button>
            </div>
        </header>
        <main>@RenderBody()</main>
        <footer class="site-footer">
            <div class="container footer-inner">
                <div class="footer-top">
                    <div class="footer-links">
                        <a href="#">About Us</a>
                        <a href="#">Contact</a>
                        <a href="#">Terms of Service</a>
                        <a href="#">Privacy Policy</a>
                    </div>
                    <div class="socials">
                        <a href="#" aria-label="Twitter">
                            <svg class="icon" width="24" height="24" viewBox="0 0 24 24" fill="currentColor"
                                 aria-hidden="true">
                                <path d="M8.29 20.251c7.547 0 11.675-6.253 11.675-11.675 0-.178 0-.355-.012-.53A8.348 8.348 0 0022 5.92a8.19 8.19 0 01-2.357.646 4.118 4.118 0 001.804-2.27 8.224 8.224 0 01-2.605.996 4.107 4.107 0 00-6.993 3.743 11.65 11.65 0 01-8.457-4.287 4.106 4.106 0 001.27 5.477A4.072 4.072 0 012.8 9.71v.052a4.105 4.105 0 003.292 4.022 4.095 4.095 0 01-1.853.07 4.108 4.108 0 003.834 2.85A8.233 8.233 0 012 18.407a11.616 11.616 0 006.29 1.84" />
                            </svg>
                        </a>
                        <a href="#" aria-label="GitHub">
                            <svg class="icon" width="24" height="24" viewBox="0 0 24 24" fill="currentColor"
                                 aria-hidden="true">
                                <path fill-rule="evenodd"
                                      d="M12 2C6.477 2 2 6.477 2 12c0 4.286 2.76 7.925 6.502 9.243.385.07.525-.166.525-.369 0-.182-.007-.82-.01-1.465-2.68.58-3.245-1.12-3.427-1.378-.145-.368-.74-1.488-1.25-1.848-.423-.298-1.02-.785-.015-.798.96-.013 1.63 1.01 1.84 1.41.05.08.103.158.155.234 1.1 1.85 2.88 1.32 3.58.995.108-.77.43-1.32.79-1.625-2.73-.31-5.59-1.36-5.59-6.075 0-1.34.48-2.43 1.27-3.285-.127-.31-.55-1.55.12-3.23 0 0 1.03-.33 3.37 1.26a11.6 11.6 0 013.06-.41c1.03.003 2.06.136 3.06.41 2.34-1.59 3.37-1.26 3.37-1.26.67 1.68.247 2.92.12 3.23.79.855 1.27 1.945 1.27 3.285 0 4.73-2.87 5.76-5.6 6.06.44.38.84 1.12.84 2.26 0 1.63-.015 3.19-.015 3.58 0 .205.14.44.53.368A10.003 10.003 0 0022 12c0-5.523-4.477-10-10-10z"
                                      clip-rule="evenodd" />
                            </svg>
                        </a>
                        <a href="#" aria-label="Facebook">
                            <svg class="icon" width="24" height="24" viewBox="0 0 24 24" fill="currentColor"
                                 aria-hidden="true">
                                <path fill-rule="evenodd"
                                      d="M22 12c0-5.523-4.477-10-10-10S2 6.477 2 12c0 4.991 3.657 9.128 8.438 9.878v-6.987h-2.54V12h2.54V9.797c0-2.506 1.492-3.89 3.777-3.89 1.094 0 2.238.195 2.238.195v2.46h-1.26c-1.243 0-1.63.771-1.63 1.562V12h2.773l-.443 2.89h-2.33v6.988C18.343 21.128 22 16.991 22 12z"
                                      clip-rule="evenodd" />
                            </svg>
                        </a>
                    </div>
                </div>
                <p class="footer-copy">© 2024 SkillSwap. All rights reserved.</p>
            </div>
        </footer>
    </div>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>